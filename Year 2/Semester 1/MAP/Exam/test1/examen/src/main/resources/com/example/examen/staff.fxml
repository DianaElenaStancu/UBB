<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.examen.controller.StaffController">
   <children>
      <TableView fx:id="tableViewPreparedId" prefHeight="256.0" prefWidth="1000.0">
        <columns>
          <TableColumn fx:id="masaPreparedId" prefWidth="105.0" text="Masa" />
          <TableColumn fx:id="dataPreparedId" prefWidth="87.0" text="Data" />
            <TableColumn fx:id="produsePreparedId" prefWidth="807.0" text="Produse" />
        </columns>
      </TableView>
      <TableView fx:id="tableViewPlacedId" layoutY="255.0" prefHeight="283.0" prefWidth="1000.0">
        <columns>
          <TableColumn fx:id="masaPlacedId" prefWidth="109.0" text="Masa" />
          <TableColumn fx:id="dataPlacedId" prefWidth="90.0" text="Data" />
            <TableColumn fx:id="produsePlacedId" prefWidth="405.0" text="Produse" />
        </columns>
      </TableView>
      <Button fx:id="prepareButtonId" layoutX="333.0" layoutY="550.0" mnemonicParsing="false" onAction="#preparedButtonOnAction" text="Prepare" />
      <Button fx:id="deliverButtonId" layoutX="500.0" layoutY="550.0" mnemonicParsing="false" onAction="#deliveredButtonOnAction" text="Deliver" />
   </children>
</AnchorPane>
